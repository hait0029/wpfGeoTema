<Window x:Class="wpfGeoTema.SuperUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wpfGeoTema"
        mc:Ignorable="d"
        Title="SuperUser" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <RadialGradientBrush>
                <GradientStop Color="#FF0A0909"/>
                <GradientStop Color="White" Offset="1"/>
                <GradientStop Color="#FF0F0E0E" Offset="0.26"/>
            </RadialGradientBrush>
        </Grid.Background>
        <Button x:Name="view_Geotemadatabase" Content="DataBaseGeoTema" HorizontalAlignment="Center" Margin="0,143,0,0" VerticalAlignment="Top" Height="128" Width="220" FontSize="24" FontWeight="Bold" Click="view_Geotemadatabase_Click"/>
        <Button x:Name="returnbtn" Content="LogOff" HorizontalAlignment="Left" Margin="39,31,0,0" VerticalAlignment="Top" Height="33" Width="88" Click="returnbtn_Click"/>

    </Grid>
</Window>
